@model JCDurigam.Models.Usuario

<div class="containerlogin">
    <div class="quadrado">
        <div class="login"><h1>Fazer Login</h1></div>
        <div class="form">
            <form asp-controller="Usuario" asp-action="Create" method="post">
                <div class="form-group">
                    <label>Nome Completo</label>
                    <input asp-for="NomeComp" class="form-control" placeholder="zezinho da silva gomes" />
                </div>
                <div class="form-group">
                    <label>Email</label>
                    <input asp-for="Email" class="form-control" placeholder="zezinho@gmail.com" />
                </div>
                <div class="form-group">
                    <label>Telefone</label>
                    <input asp-for="Telefone" class="form-control" placeholder="11123456789" />
                   
                </div>

                <div class="botaopqp">
                <button type="submit" class="btnhehe">Salvar</button>
                </div>
               
            </form>
        </div>
    </div>
</div>

